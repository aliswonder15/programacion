public class Aliss {
    public static void main(String[] args) {
        Cliente cliente = new Cliente();
        cliente.edad = 27;
        System.out.println(cliente.edad);
        cliente.nombre = "Aliss";
        System.out.println(cliente.nombre);
        cliente.telefono = 310216827;
        System.out.println(cliente.telefono);
        cliente.credito = -500;
        System.out.println(cliente.credito);
        Trabajador trabajador = new Trabajador();
        trabajador.salario = 1000;
        System.out.println(trabajador.salario);
        trabajador.edad = 40;
        System.out.println(trabajador.edad);
        trabajador.nombre = "RÃº";
        System.out.println(trabajador.nombre);
        trabajador.telefono = 66666666;
        System.out.println(trabajador.telefono);
        trabajador.credito = 200;
        System.out.println(trabajador.credito);


    }
}
class Person{
    int edad;
    String nombre;
    long telefono;
}
class Cliente extends Person{
    int credito;
}
class Trabajador extends Cliente{
    int salario;
}
